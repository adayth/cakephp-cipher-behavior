{
    "name": "adayth/cakephp-cipher-behavior",
    "description": "Secure your DB data with CakePHP ORM and this behavior",
    "keywords": ["cakephp", "plugin", "cipher", "cypher", "crypt", "encrypt", "behaviors", "decrypt", "PCI Compliance", "orm"],
    "homepage": "https://github.com/adayth/cakephp-cipher-behavior",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Aday Talavera Hierro",
            "email": "aday.talavera@gmail.com",
            "homepage": "http://adaytalavera.es"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/orm": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "4.*"
    },
    "autoload": {
        "psr-4": {
            "CipherBehavior\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CipherBehavior\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
